{
    "name": "whatwedo/crud-bundle",
    "type": "symfony-bundle",
    "description": "Very simple CRUD operations",
    "license": "MIT",
    "homepage": "https://doc.whatwedo.ch/whatwedo/crudbundle",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Ueli Banholzer",
            "email": "ueli@whatwedo.ch"
        },
        {
            "name": "Felix Imobersteg",
            "email": "felix@whatwedo.ch"
        },
        {
            "name": "Nicolo Singer",
            "email": "nicolo@whatwedo.ch"
        }
    ],
    "require": {
        "php":                      ">=8.0",
        "symfony/framework-bundle": "~4.0|~5.0",
        "symfony/http-kernel":      "~4.0|~5.0",
        "symfony/security-bundle":  "~4.0|~5.0",
        "symfony/serializer":       "~4.0|~5.0",
        "symfony/form":             "~4.0|~5.0",
        "symfony/twig-bundle":      "~4.0|~5.0",
        "whatwedo/core-bundle":     "^0.5",
        "whatwedo/table-bundle":    "^0.5",
        "phpspec/prophecy": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|~5.0"
    },
    "suggest": {
        "mhujer/breadcrumbs-bundle": "Allows creation of breadcrumbs"
    },
    "autoload": {
        "psr-4": {
            "whatwedo\\CrudBundle\\": ""
        }
    }
}
